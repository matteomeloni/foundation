{
    "name": "matteomeloni/foundation",
    "description": "A laravel package that provide a basis for api ",
    "license": "MIT",
    "authors": [
        {
            "name": "Matteo Meloni",
            "email": "matteomelonig@gmail.com",
            "homepage": "https://github.com/matteomeloni"
        }
    ],
    "homepage": "https://github.com/matteomeloni/foundation",
    "keywords": ["Laravel", "Foundation"],
    "require": {
        "illuminate/support": "~9"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "Matteomeloni\\Foundation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Matteomeloni\\Foundation\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Matteomeloni\\Foundation\\FoundationServiceProvider",
                "Matteomeloni\\Foundation\\FoundationResponseMacroServiceProvider"
            ],
            "aliases": {
                "Foundation": "Matteomeloni\\Foundation\\Facades\\Foundation"
            }
        }
    }
}
